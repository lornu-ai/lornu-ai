{
    "name": "lornu-ai-web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "kill": "fuser -k 5000/tcp",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:integration": "vitest run src/**/*.integration.test.tsx",
        "test:ci": "vitest run --coverage",
        "api": "cd ../.. && uv run -- uvicorn packages.api.main:app --reload --port 8080",
        "test:e2e": "start-server-and-test api http://127.0.0.1:8080/docs dev http://localhost:5174 \"playwright test\"",
        "test:e2e:ui": "start-server-and-test api http://127.0.0.1:8080/docs dev http://localhost:5174 \"playwright test --ui\"",
        "test:e2e:smoke": "playwright test tests/e2e/smoke-test.spec.ts --project=chromium",
        "dev:k8s": "kustomize build ../../kubernetes/overlays/lornu-dev | kubectl apply -f -"
    },
    "dependencies": {
        "@github/spark": "^0.39.0",
